[Unit]
Description=tarotclubweb

[Service]
WorkingDirectory=/var/www/www.tarotclub.fr
ExecStart=/opt/node-v0.12.4-linux-x64/bin/node /var/www/www.tarotclub.fr/app.js
Restart=alway
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=tarotclubweb
Environment='PATH=/usr/bin:/usr/local/bin:/opt/node-v0.12.4-linux-x64/bin'
Environment='NODE_ENV=production'

[Install]
WantedBy=multi-user.target

